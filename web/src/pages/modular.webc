---js
{
  layout: 'base.webc',
  eleventyComputed: {
    data: data => data.item,
    description: data => data.item.meta.description,
    permalink: data => `${data.item.meta.slug || ''}/index.html`,
    title: data => data.item.meta.title,
  },
  pagination: {
    data: 'sanity.modular',
    size: 1,
    alias: 'item',
  }
}
---
<main class="container stack-m" @html="renderHTML(item.content)"></main>
